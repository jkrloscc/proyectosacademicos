        <activity
            android:name="com.example.chatrcmmappii.ChatRCMMAppIIActivity"
            android:icon="@drawable/chaticon"
            android:label="@string/title_activity_chat_rcmmappii"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.chatrcmmappii.ChatRCMMAppIIListActivity"
            android:icon="@drawable/chaticon"
            android:label="@string/title_activity_chat_rcmmappii_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.chatrcmmappii.ChatRCMMAppIIListActivityGrupo"
            android:icon="@drawable/chaticon"
            android:label="@string/title_activity_chat_rcmmappii_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.chatrcmmappii.AudioRecord"
            android:icon="@drawable/chaticon"
            android:label="@string/title_activity_chat_rcmmappii_list"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>